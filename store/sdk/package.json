{
  "debug": true,
  "main": "dev.sys.js",
  "browserArguments": "--disable-web-security --kiosk-printing --allow-running-insecure-content --no-displaying-insecure-content --ignore-certificate-errors",
  "nodeArguments": "--trace-deprecation",
  "pageShow": true,
  "page": "http://localhost:5173/",
  "name": "sys-shim",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint ./ --format unix",
    "lint-fix": "eslint ./ --fix --format unix",
    "dev": "run-p main:run vite:run",
    "main:run": "yarn main:run-shim",
    "main:run-shim": "win-api/dist/main.exe a b c d=1 /e 3",
    "main:build": "npx download-cli https://github.com/wll8/sys-shim/releases/download/example/main.exe -o win-api/dist",
    "vite:run": "vite",
    "vite": "vite",
    "preview": "vite preview",
    "sys": "node -r esm build.js",
    "sys:dev": "node -r esm build.js development",
    "prepare": "husky"
  },
  "env": {
    "node": "18.19.0"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "proxy-deep": "^3.1.1",
    "rpc-websockets": "^7.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "download-cli": "^1.1.1",
    "eslint": "8.57.0",
    "esm": "^3.2.25",
    "husky": "^9.0.11",
    "shx": "^0.3.4",
    "vite": "^5.0.0",
    "vue": "^3.3.8",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
