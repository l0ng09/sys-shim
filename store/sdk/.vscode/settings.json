{
  "cSpell.words": [
    "CCOMMIT",
    "CMETHODS",
    "CVERSION",
    "exmode",
    "fsys",
    "hwnd",
    "inet",
    "LBUTTONUP",
    "neutralino",
    "neutralinojs",
    "popen",
    "RBUTTONUP",
    "Tauri",
    "WSAVSTLOADED"
  ],
  // Disable the default formatter, use eslint instead
  "prettier.enable": false,
  "editor.formatOnSave": false,

  // Auto fix
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  }
}
